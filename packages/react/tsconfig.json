{
    "compilerOptions": {
        "target": "ES2019",
        "module": "esnext",
        "declaration": true,
        "outDir": "dist",
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "jsx": "react-jsx",
        "jsxImportSource": "react"
    },
    "include": ["src"]
}
