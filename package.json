{
  "name": "ai-knowledge-search",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=18.17"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:web\" \"npm run dev:api\"",
    "dev:web": "vite dev --host 0.0.0.0",
    "dev:api": "tsx watch src/api/index.ts",
    "build": "vite build && tsup src/server/index.ts --format esm --out-dir dist/server",
    "start": "vite start",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
