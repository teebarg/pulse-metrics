{
    "name": "@pulsemetrics/sdk",
    "version": "1.0.0",
    "description": "Vanilla JavaScript SDK for PulseMetrics",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.esm.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.esm.js",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "rollup -c",
        "dev": "rollup -c -w",
        "test": "vitest",
        "test:watch": "vitest --watch",
        "clean": "rm -rf dist",
        "prepublishOnly": "pnpm run build"
    },
    "keywords": [
        "analytics",
        "tracking",
        "pulsemetrics"
    ],
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^15.2.0",
        "@rollup/plugin-typescript": "^11.1.0",
        "@rollup/plugin-terser": "^0.4.4",
        "rollup": "^3.29.0",
        "typescript": "^5.0.0",
        "vitest": "^1.0.0"
    },
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    }
}
