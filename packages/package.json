{
    "name": "pulsews",
    "version": "1.0.1",
    "description": "PulseWS â€” A robust, easy-to-use WebSocket SDK for real-time apps.",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "clean": "rm -rf dist",
        "prepare": "npm run build",
        "build": "tsup src/index.ts --format cjs,esm --dts",
        "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
        "lint": "tsc --noEmit"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/teebarg/pulse-ws.git"
    },
    "keywords": [
        "websocket",
        "ws",
        "realtime",
        "sdk",
        "typescript",
        "react",
        "node"
    ],
    "author": "Adeniyi Aderounmu <teebarg01@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/teebarg/pulse-ws/issues"
    },
    "homepage": "https://github.com/teebarg/pulse-ws#readme",
    "devDependencies": {
        "@types/react": "^19.2.7",
        "react": "^19.2.0",
        "tsup": "^8.5.1",
        "typescript": "^5.1.3"
    }
}
