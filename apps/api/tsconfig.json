{
  "compilerOptions": {
    "target": "ESNext",
    "module": "preserve",
    "strict": true,
    "moduleResolution": "bundler",
    "verbatimModuleSyntax": true,
    "skipLibCheck": true,
    "types": [
      "node"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "outDir": "./dist",
    "paths": {
            "~/*": ["./*"]
        }
  },
  "exclude": ["node_modules"]
}
