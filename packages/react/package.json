{
    "name": "@pulsemetrics/react",
    "version": "1.0.0",
    "description": "React SDK for PulseMetrics",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.esm.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.esm.js",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "rollup -c",
        "dev": "rollup -c -w",
        "test": "vitest",
        "clean": "rm -rf dist",
        "prepublishOnly": "pnpm run build"
    },
    "peerDependencies": {
        "react": ">=18",
        "react-dom": ">=18"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^29.0.0",
        "@rollup/plugin-node-resolve": "^15.2.0",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^11.1.0",
        "@testing-library/react": "^14.0.0",
        "@types/react": "^18.3.27",
        "@types/react-dom": "^19.2.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rollup": "^3.29.0",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "typescript": "^5.0.0",
        "vitest": "^1.0.0"
    },
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    }
}
