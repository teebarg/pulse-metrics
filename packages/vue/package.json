{
    "name": "@pulsemetrics/vue",
    "version": "1.0.0",
    "description": "Vue 3 SDK for PulseMetrics",
    "main": "./dist/index.js",
    "module": "./dist/index.esm.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.esm.js",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "rollup -c",
        "dev": "rollup -c -w",
        "test": "vitest",
        "clean": "rm -rf dist",
        "prepublishOnly": "pnpm run build"
    },
    "peerDependencies": {
        "vue": "^3.0.0"
    },
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^15.2.0",
        "@rollup/plugin-typescript": "^11.1.0",
        "@vue/test-utils": "^2.4.0",
        "rollup": "^3.29.0",
        "typescript": "^5.0.0",
        "vue": "^3.3.0",
        "vitest": "^1.0.0"
    },
    "license": "MIT"
}
